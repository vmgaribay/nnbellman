{
    "name": "SchemeB",
    "n_gpu": 1,
    "seed": 21,


    "arch": {
        "type": "SixLayerPudge",
        "args": {
            "input_dim":4, 
            "output_dim":2,
            "max_nodes":2048}
    },
    "data_loader": {
        "type": "BellmanDataLoader",
        "args":{
            "data_dir": "data/",
            "input_csv_file":"DatasetJan/AgentDataEfficacy-29Jan2025.csv", 
            "input_scale":{"alpha":{"dist":"norm","params":[1.08,0.074]},"k":{"dist":"unif","params":[0,40]},"sigma":{"dist":"unif","params":[0,2]},"theta":{"dist":"unif","params":[0,1]}},
            "output_csv_file":"DatasetJan/ResultsFinalEfficacy-29Jan2025clean.csv",
            "categories":{"N":0.0,"L":0.2,"H":0.5},
            "output_variable":"both",
            "scale_output": "both",
            "batch_size": 64,
            "shuffle": true,
            "validation_split": 0.2,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "mse_loss",
    "metrics": [
        "model_mse", "model_mae", "model_max_ae","consumption_mae","consumption_max_ae","consumption_mpe","consumption_max_pe","i_a_mae","i_a_max_ae","i_a_mpe","i_a_max_pe","n_wrong_i_a"
    ],
    "possible_i_a":[0.0,0.2,0.5],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 200,

        "save_dir": "saved/SchemeB",
        "save_period": 50,
        "verbosity": 1,
        
        "monitor": "min val_loss",
        "early_stop": 20,

        "tensorboard": true
    }
}
