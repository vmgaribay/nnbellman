{
    "name": "TestModel",
    "n_gpu": 1,
    "seed": 21,


    "arch": {
        "type": "ThreeLayer",
        "args": {
            "input_dim":4, 
            "output_dim":2,
            "max_nodes":512}
    },
    "data_loader": {
        "type": "BellmanDataLoader",
        "args":{
            "data_dir": "data/",
            "input_csv_file":"DatasetOct/AgentData-Updated12Oct2024.csv", 
            "output_csv_file":"DatasetOct/ResultsFinal-Updated12Oct2024clean.csv",
            "categories":{"N":0.0,"L":0.25,"H":0.45},
            "output_variable":"both",
            "scale_output": "both",
            "batch_size": 512,
            "shuffle": true,
            "validation_split": 0.2,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "mse_loss",
    "metrics": [
        "model_mae", "model_max_ae","consumption_mae","consumption_max_ae","i_a_mae","i_a_max_ae","n_wrong_i_a","n_exceeding_i_a_k" 
    ],
    "possible_i_a":[0.0,0.25,0.45],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 200,

        "save_dir": "saved/both/S21",
        "save_period": 50,
        "verbosity": 1,
        
        "monitor": "min val_loss",
        "early_stop": 15,

        "tensorboard": true
    }
}
